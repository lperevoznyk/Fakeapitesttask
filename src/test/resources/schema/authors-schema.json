{
  "$schema": "http://json-schema.org/draft-07/schema#",

  "title": "Authors list",
  "type": "array",

  "items": {
    "type": "object",
    "additionalProperties": false,

    "properties": {
      "id":      { "type": "integer" },
      "idBook":  { "type": "integer" },

      "firstName": { "type": ["string", "null"] },
      "lastName":  { "type": ["string", "null"] }
    },

    "required": [ "id", "idBook" ]
  }
}
